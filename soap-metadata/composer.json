{
  "name": "goetas-webservices/soap-common",
  "authors": [
    {
      "name": "Asmir Mustafic",
      "email": "goetas@gmail.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "psr/http-message": "^1.0",
    "rybakit/arguments-resolver": "^0.5.0",
    "php-http/discovery": "^1.0",
    "php-http/message-factory": "^1.0.2",
    "doctrine/instantiator": "^1.0.3",
    "goetas-webservices/xsd2php-runtime": "^0.2.11",
    "jms/serializer": "^1.6|^2.0|^3.0"
  },
  "require-dev": {
    "php-http/message": "^1.0",
    "guzzlehttp/psr7": "^1.3",
    "phpunit/phpunit": "^4.8|^5.0",
    "goetas-webservices/xsd2php": "^0.4.0@dev",
    "goetas-webservices/wsdl2php": "^v0.4.4@dev"
  },
  "autoload": {
    "psr-4": {
