{
  "name": "goetas-webservices/soap-server",
  "authors": [
    {
      "name": "Asmir Mustafic",
      "email": "goetas@gmail.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "rybakit/arguments-resolver": "^0.5.0",
    "jms/serializer": "^1.2|^2.0|^3.0",
    "psr/log": "^1.0",
    "psr/http-message": "^1.0",
    "psr/http-server-handler": "^1.0",
    "psr/http-factory": "^1.0",
    "php-http/discovery": "^1.6",
    "psr/http-factory-implementation": "^1.0",
    "psr/http-message-implementation": "^1.0"
  },
  "require-dev": {
    "guzzlehttp/psr7": "^1.3",
    "phpunit/phpunit": "^7.0|^8.0",
    "goetas-webservices/xsd2php": "^0.4.0@dev",
    "goetas-webservices/wsdl2php": "^v0.4.4@dev",
    "laminas/laminas-diactoros": "^2.4",
    "doctrine/coding-standard": "^8.1",
    "laminas/laminas-httphandlerrunner": "^1.3",
    "goetas-webservices/xsd2php-runtime": "^0.2.13"
  },
  "suggest": {
    "php-http/discovery": "^1.6"
  },
  "autoload": {
    "psr-4": {
      "GoetasWebservices\\SoapServices\\SoapServer\\": "src",
      "GoetasWebservices\\SoapServices\\Metadata\\": "soap-metadata/src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "GoetasWebservices\\SoapServices\\SoapServer\\Tests\\": "tests",
      "GoetasWebservices\\Xsd\\XsdToPhp\\Tests\\": "vendor/goetas-webservices/xsd2php/tests",
      "GoetasWebservices\\WsdlToPhp\\Tests\\": "vendor/goetas-webservices/wsdl2php/tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1-dev"
    }
  }
}
